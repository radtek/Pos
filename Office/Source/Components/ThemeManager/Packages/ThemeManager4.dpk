  Vclx40,
  Vcldb40;

contains
  ThemeMgr in '..\ThemeMgr.pas',
  ThemeMgrDB in '..\ThemeMgrDB.pas',
  ThemeSrv in '..\ThemeSrv.pas',
  TmSchema in '..\TmSchema.pas',
  UxTheme in '..\UxTheme.pas';

end.

